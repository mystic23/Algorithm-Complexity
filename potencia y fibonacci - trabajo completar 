    {
      "cell_type": "code",
      "source": [
        "import time\n",
        "\n",
        "def fibonacci_iterative_(n):\n",
        "    fib_sequence = []\n",
        "\n",
        "    if n >= 1:\n",
        "        fib_sequence.append(0)\n",
        "    if n >= 2:\n",
        "        fib_sequence.append(1)\n",
        "\n",
        "    for i in range(2, n):\n",
        "        fib_sequence.append(fib_sequence[i - 1] + fib_sequence[i - 2])\n",
        "\n",
        "    return fib_sequence\n",
        "\n",
        "start_time = time.time()\n",
        "result = fibonacci_iterative_(40)\n",
        "end_time = time.time()\n",
        "\n",
        "print(\"Fibonacci sequence:\", result)\n",
        "print(\"Time taken:\", end_time - start_time, \"seconds\")\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "ldSzmUQlgTa4",
        "outputId": "ebb433b3-bba6-4dec-99cb-cf7912386cdf"
      },
