      "source": [
        "#Este es usando con productoria normal"
      ],
      "metadata": {
        "id": "hQURF8yOcHTo"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def pot_or(a, n):\n",
        "    result = 1\n",
        "    for _ in range(n):\n",
        "        result *= a\n",
        "    return result\n",
        "\n",
        "print(pot_or(2,10))\n",
        "\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
