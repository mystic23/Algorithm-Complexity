        "#Con times"
      ],
      "metadata": {
        "id": "lGkFiTgnc8vx"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import time\n",
        "\n",
        "def pot_or(a, n):\n",
        "    result = 1\n",
        "    for _ in range(n):\n",
        "        result *= a\n",
        "    return result\n",
        "\n",
        "base = float(input(\"Ingresa la base (a): \"))\n",
        "exponente = int(input(\"Ingresa el exponente (n): \"))\n",
        "\n",
        "start_time = time.time()\n",
        "resultado = pot_or(base, exponente)\n",
        "end_time = time.time()\n",
        "\n",
        "print(f\"{base} elevado a la potencia {exponente} es igual a {resultado}\")\n",
        "print(f\"Tiempo de ejecución: {end_time - start_time} segundos\")\n",
        "recursivo = end_time - start_time"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "0dOHWb8Qc-x-",
        "outputId": "15fdf10e-cc03-49a1-b2b5-0168dc4002ef"
      },
      "execution_count": 9,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Ingresa la base (a): 2\n",
            "Ingresa el exponente (n): 100000\n",
            "2.0 elevado a la potencia 100000 es igual a inf\n",
            "Tiempo de ejecución: 0.009067535400390625 segundos\n"
          ]
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "import time\n",
        "\n",
        "def pot_recursive(x, n):\n",
        "    if n == 1:\n",
        "        return x\n",
        "    else:\n",
        "        if n % 2 == 0:\n",
        "            return pot_recursive(x, n//2) * pot_recursive(x, n//2)\n",
        "        else:\n",
        "            return pot_recursive(x, (n+1)//2) * pot_recursive(x, (n-1)//2)\n",
        "\n",
        "def pot_with_loop(a, n):\n",
        "    result = 1\n",
        "    for _ in range(n):\n",
        "        result *= a\n",
        "    return result\n",
        "\n",
        "min_exponent = 1\n",
        "max_exponent = 1000\n",
        "base = 2\n",
        "\n",
        "for exponent in range(min_exponent, max_exponent + 1):\n",
        "    start_time_rec = time.time()\n",
        "    pot_recursive(base, exponent)\n",
        "    end_time_rec = time.time()\n",
        "\n",
        "    start_time_loop = time.time()\n",
        "    pot_with_loop(base, exponent)\n",
        "    end_time_loop = time.time()\n",
        "\n",
        "    if (end_time_rec - start_time_rec) > (end_time_loop - start_time_loop):\n",
        "        print(f\"Para exponente {exponent}, la implementación con bucle es más rápida.\")\n",
        "    else:\n",
        "        print(f\"Para exponente {exponent}, la implementación recursiva es más rápida.\")\n"
      ],
      "metadata": {
