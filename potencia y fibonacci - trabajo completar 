      "source": [
        "def pot(x,n):\n",
        "  if n==1:\n",
        "    return x\n",
        "  else:\n",
        "    if n%2 == 0:\n",
        "      return pot(x,n/2)*pot(x,n/2)\n",
        "    else:\n",
        "      return pot(x,(n+1)/2)*pot(x,(n-1)/2)\n",
        "\n",
        "\n",
        "print(pot(2,10))\n",
        "\n"
      ]
    },
    {
      "cell_type": "markdown",
      "source": [
        "#Con el times"
      ],
      "metadata": {
        "id": "wMYgkIS4cwcC"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import time\n",
        "\n",
        "def pot(x, n):\n",
        "    if n == 1:\n",
        "        return x\n",
        "    else:\n",
        "        if n % 2 == 0:\n",
        "            return pot(x, n//2) * pot(x, n//2)\n",
        "        else:\n",
        "            return pot(x, (n+1)//2) * pot(x, (n-1)//2)\n",
        "\n",
        "base = float(input(\"Ingresa la base (x): \"))\n",
        "exponente = int(input(\"Ingresa el exponente (n): \"))\n",
        "\n",
        "start_time = time.time()\n",
        "resultado = pot(base, exponente)\n",
        "end_time = time.time()\n",
        "\n",
        "print(f\"{base} elevado a la potencia {exponente} es igual a {resultado}\")\n",
        "print(f\"Tiempo de ejecución: {end_time - start_time} segundos\")\n",
        "normal =\n",
        "end_time -start_time\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
